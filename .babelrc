{
    "presets": [
        [
            "env",
            {
                "modules": false,
                "targets": {
                    "browsers": [
                        "> 5%",
                        "last 2 versions",
                        "not ie <= 8"
                    ]
                }
            }
        ],
        "es2015",
        "node6"
    ],
    "plugins": ["transform-runtime", "transform-class-properties", "syntax-dynamic-import"]
}
